{
  "name": "pg-arango-migration-app",
  "version": "1.0.0",
  "description": "An app for transforming postgres to arango databases",
  "main": "./public/main.js",
  "scripts": {
    "start": "concurrently \"npm run react-start\" \"npm run server-start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-start": "electron .",
    "react-build": "react-scripts build",
    "react-start": "BROWSER=none react-scripts start",
    "server-build": "babel server --out-dir build-server",
    "server-start": "nodemon --watch server --exec babel-node server/server.js"
  },
  "keywords": [
    "Electron",
    "React",
    "Node",
    "Arango",
    "Postgres"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.5",
    "@babel/node": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "concurrently": "^7.2.2",
    "electron": "^17.0.0",
    "nodemon": "^2.0.15",
    "react-scripts": "^5.0.1",
    "wait-on": "^6.0.1"
  },
  "dependencies": {
    "@types/jest": "^28.1.3",
    "@types/node": "^18.11.10",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "allotment": "^1.15.0",
    "app-module-path": "^2.2.0",
    "axios": "^0.27.2",
    "bootstrap": "5.2.0-beta1",
    "classnames": "^2.3.1",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "final-form": "^4.20.7",
    "pg-arango-transform": "file:../pg-arango-transform",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-final-form": "^6.5.9",
    "react-graph-vis": "^1.0.7",
    "react-router-dom": "^6.3.0",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.5.1",
    "typescript": "^4.7.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
